    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "S4D\\Laravel\\IsmartSMS\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Abdulaziz Al Rashdi",
